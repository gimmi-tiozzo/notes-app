    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Note App JS",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/notes-js/src/app.js",
            "args": ["list"]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Note App Typescript",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/notes-typescript/dist/app.js",
            "args": ["list"]
        }
    ]
}
